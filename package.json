{
	"name": "career-aptitude-explorer",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
		"start": "cd backend && node dist/app.js",
		"test:render": "npm run build && npm start",
		"build": "cd frontend && npm install && npm run build && cd ../backend && npm install && npm run build",
		"build:backend": "cd backend && npm install && npm run build",
		"build:frontend": "cd frontend && npm install && npm run build",
		"server:dev": "cd backend && npm run dev",
		"client:dev": "cd frontend && npm run dev"
	},
	"dependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.7",
		"axios": "^1.7.9",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"express": "^4.18.2"
	},
	"devDependencies": {
		"@types/react": "^18.2.48",
		"@types/react-dom": "^18.2.18",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^20.11.0",
		"@types/pg": "^8.11.0",
		"@types/sequelize": "^4.28.20",
		"concurrently": "^8.2.2",
		"typescript": "^5.7.3",
		"vite": "^6.0.7"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
