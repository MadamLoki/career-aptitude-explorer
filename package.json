{
	"name": "career-aptitude-explorer",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
		"start": "npm run build && concurrently \"npm run server\" \"npm run client:start\"",
		"build": "npm run install:all && npm run client:build && npm run server:build",
		"server": "cd backend && npm run start",
		"server:dev": "cd backend && npm run dev",
		"server:build": "cd backend && npm run build",
		"client:dev": "cd frontend && npm run dev",
		"client:start": "cd frontend && npm run start",
		"client:build": "cd frontend && npm run build",
		"install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
		"postinstall": "npm run build"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^20.11.0",
		"@types/pg": "^8.11.0",
		"@vitejs/plugin-react": "^4.2.1",
		"concurrently": "^8.2.2",
		"nodemon": "^3.0.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.12"
	},
	"dependencies": {
		"@heroicons/react": "^2.1.1",
		"@material-tailwind/react": "^2.1.8",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.11.3",
		"react-feather": "^2.0.10",
		"sequelize": "^6.35.2"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}